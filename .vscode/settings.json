{
    "emmet.includeLanguages": {
        "rust": "html",
        "*.rs": "html"
    },
    "tailwindCSS.includeLanguages": {
        "rust": "html",
        "*.rs": "html"
    },
    "files.associations": {
        "*.rs": "rust"
    },
    "editor.quickSuggestions": {
        "other": "on",
        "comments": "on",
        "strings": true
    },
    "css.validate": false,
    "i18n-ally.localesPaths": [
        "locales"
    ],
    "rust-analyzer.cargo.features": [
        "ssr"
    ],
    "i18n-ally.keystyle": "nested"
}